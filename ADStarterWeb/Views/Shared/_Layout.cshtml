<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_Style" />
</head>
@if (ViewData["Title"] == "Dashboard")
{
    <body onload="startTime()">
        <partial name="_Loader" />
        <div class="tap-top"><i data-feather="chevrons-up"></i></div>
        <div class="page-wrapper compact-wrapper" id="pageWrapper">
            <div class="page-body-wrapper sidebar-icon">
                <partial name="_Header" />
                <div class="page-body-wrapper">
                    @if (ViewContext.RouteData.Values["area"]?.ToString() == "Parent")
                    {
                        @await Html.PartialAsync("_SidebarParent")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "Therapist")
                    {
                        @await Html.PartialAsync("_SidebarTherapist")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "CustomerService")
                    {
                        @await Html.PartialAsync("_SidebarCustomerService")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "Admin")
                    {
                        @await Html.PartialAsync("_SidebarAdmin")
                    }
                    else
                    {
                        <partial name="_SidePenal" />
                    }
                    @* <partial name="_Notifications" /> *@
                    @RenderBody()
                    <partial name="IconMarkup" />
                    <partial name="_Footer" />
                </div>
            </div>
        </div>
        <partial name="_Script" />
        @RenderSection("Script", false)

        <script src="../assets/js/theme-customizer/customizer.js"></script>
        @* @RenderSection("Script", required: false)
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
        <script src="~/assets/js/bootstrap/bootstrap.bundle.min.js"></script>
        <script src="~/assets/js/icons/feather-icon/feather.min.js"></script>
        <script src="~/assets/js/icons/feather-icon/feather-icon.js"></script>
        <script src="~/assets/js/config.js"></script>
        <script src="~/assets/js/script.js"></script> *@

    </body>
}
else
{
    <body>
        <partial name="_Loader" />
        <div class="tap-top"><i data-feather="chevrons-up"></i></div>
        <div class="page-wrapper compact-wrapper" id="pageWrapper">
            <div class="page-body-wrapper sidebar-icon">
                <partial name="_Header" />
                <div class="page-body-wrapper">
                    @if (ViewContext.RouteData.Values["area"]?.ToString() == "Parent")
                    {
                        @await Html.PartialAsync("_SidebarParent")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "Therapist")
                    {
                        @await Html.PartialAsync("_SidebarTherapist")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "CustomerService")
                    {
                        @await Html.PartialAsync("_SidebarCustomerService")
                    }
                    else if (ViewContext.RouteData.Values["area"]?.ToString() == "Admin")
                    {
                        @await Html.PartialAsync("_SidebarAdmin")
                    }
                    else
                    {
                        <partial name="_SidePenal" />
                    }
                    @RenderBody()
                    <partial name="_Footer" />
                </div>
            </div>
        </div>
        <partial name="_Script" />
        @RenderSection("Script", false)

        <script src="../assets/js/theme-customizer/customizer.js"></script>
        @* @if (IsSectionDefined("scripts"))
        {
            RenderSection("scripts");
        }
        <script src="../assets/js/sidebar-menu.js"></script>
        <script src="../assets/js/slick/slick.min.js"></script>
        <script src="../assets/js/slick/slick.js"></script>
        <script src="../assets/js/header-slick.js"></script>
        <!-- Plugins JS Ends-->
        <!-- Theme js-->
        <!-- Plugins JS Ends-->
        <!-- Theme js-->
        <script src="../assets/js/script.js"></script>
        <!-- Plugins JS start-->
        <script src="../assets/js/dropzone/dropzone.js"></script>
        <script src="../assets/js/dropzone/dropzone-script.js"></script>
        <script src="../assets/js/tooltip-init.js"></script>
        <!-- Plugins JS Ends-->
        <!-- Theme js-->
        <script src="../assets/js/script.js"></script>
        @RenderSection("Script", required: false) *@
    </body>
}
</html>
